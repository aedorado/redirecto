{
   "description": "Setup custom redirect rules for any http request i.e pages, scripts, images etc. Uses a simple string replace to do this.",
   "manifest_version": 2,
   "name": "Redirecto",
   "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
   "background": {
      "scripts": ["bg.js"]
   },
   "browser_action": {
      "default_icon": "icons/icon16.png",
      "default_popup": "popup.html",
      "default_title": "Add Redirects."
   },
   "permissions": [ "webRequest", "webRequestBlocking", "\u003Call_urls>", "tabs" ],
   // "update_url": "",
   "version": "1.33"
}
